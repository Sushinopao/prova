#include <stdio.h>

int main()
{
    
    int contador, num, resultado, x=0, y=1;
    printf("digite um numero da serie de Fibonacci: ");
    scanf("%d", &num);
    printf("1, ");
    for(contador = 1; contador < num; contador++){
        resultado = x+y;
        x=y;
        y=resultado;
        printf("%d, ", resultado);
    }

    return 0;
}
